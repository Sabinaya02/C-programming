// Online C compiler to run C program online
#include <stdio.h>
#include<string.h>
int main() {
    char str[50];
    scanf("%s",str);
    for(int i=0;str[i]!='\0';i++){
        if(str[i]>=65 && str[i]<=90){=>str[i]>=97 && str[i]<=122
            str[i]=str[i]+32;=>str[i]=str[i]-32;--->to print lower to upper
        }
    }
    printf("%s",str);
}
    
