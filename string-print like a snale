// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a;
    scanf("%d ",&a);
    int arr[a][a];
    for(int i=0;i<a;i++){
        for(int j=0;j<a;j++){
            scanf("%d",&arr[i][j]);
        }
}
       for(int i=0;i<a;i++){
      if(i%2==0){
    for(int j=0;j<a;j++){
        printf("%d",arr[i][j]);
    }
      }
      else{
         for(int j=a-1;j>=0;j--){
    printf("%d",arr[i][j]);
}
}
}
}
